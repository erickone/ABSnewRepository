<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
 
    <application>
        <variable-resolver>org.springframework.web.jsf.DelegatingVariableResolver</variable-resolver>
    </application>
 
    <navigation-rule>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.abs</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>returnlogin</from-outcome>
            <to-view-id>/login.abs</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>profiles</from-outcome>
            <to-view-id>/autenticado/optionsProfile.abs</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>options</from-outcome>
            <to-view-id>/autenticado/options.abs</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>newDraftProject</from-outcome>
            <to-view-id>/autenticado/ppp/draftProject.abs</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>newInPreFile</from-outcome>
            <to-view-id>/autenticado/ppp/invPreFile.abs</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>DepToObjLink</from-outcome>
            <to-view-id>/autenticado/ppp/DepToObjLink.abs</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>findPreFicha</from-outcome>
            <to-view-id>/autenticado/ppp/InvPrfeFileSearch.abs</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>validationImportCeilingbBdget</from-outcome>
            <to-view-id>/autenticado/ppp/components/Techos/validationImportCeilingBudget.abs</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>printErrorImportCeilingBudget</from-outcome>
            <to-view-id>/autenticado/ppp/components/Techos/printErrorImportCeilingBudget.abs</to-view-id>
            <redirect/>
        </navigation-case>        
        <navigation-case>
            <from-outcome>importCeilingBudget</from-outcome>
            <to-view-id>/autenticado/ppp/components/Techos/importCeilingBudget.abs</to-view-id>
            <redirect/>
        </navigation-case>  
    </navigation-rule>
    <application>  
        <locale-config>
            <default-locale>es_MX</default-locale>
        </locale-config>
    </application>  
    <application>
        <resource-bundle>
            <base-name>com.abs.siif.ppp.resources.msgExcepUI</base-name>
            <var>ExceptionMsg</var>
        </resource-bundle>
    </application>
    <application>
        <resource-bundle>
            <base-name>com.abs.siif.ppp.resources.msgUI</base-name>
            <var>InfoMsg</var>
        </resource-bundle>
    </application>
</faces-config>